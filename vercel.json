{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/geminiProxy.ts": {
      "maxDuration": 30
    },
    "api/claudeProxy.ts": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "VITE_BUILD_TIME": "true"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}




